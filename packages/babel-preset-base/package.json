{
  "name": "@wpw/babel-preset-base",
  "version": "0.0.0",
  "description": "Babel preset for @wps/scripts, without react specific transforms.",
  "main": "lib/index.js",
  "repository": "https://github.com/swashata/wp-webpack-script",
  "author": "Swashata Ghosh",
  "license": "MIT",
  "private": false,
  "devDependencies": {
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src --out-dir lib --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "watch": "babel -w src --out-dir lib --ignore **/__tests__"
  },
  "publishConfig": {
    "access": "public"
  }
}
